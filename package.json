{
  "name": "rr-v4-ssr-ts",
  "version": "0.0.2",
  "description": "Demo for SSR with rr@v4 and rrr@v5 using TypeScript",
  "scripts": {
    "build": "webpack -p --optimize-minimize --define NODE_ENV=production",
    "dev": "rimraf ./dist/* && cross-env NODE_ENV=development cross-env ENV_CONF=local cross-env PORT=3005 ts-node ./server/server.ts",
    "prod": "cross-env NODE_ENV=production PORT=3005 node ./dist/server.js",
    "start": "rimraf ./dist/* && npm run build && npm run prod"
  },
  "main": "index.js",
